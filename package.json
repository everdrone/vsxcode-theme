{
    "name": "vsxcode",
    "displayName": "XCode Catalina Theme",
    "description": "VSCode theme inspired by the dark and light version of XCode's catalina theme",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.50.0"
    },
    "categories": [
        "Themes"
    ],
    "contributes": {
        "themes": [
            {
                "label": "Catalina Dark",
                "uiTheme": "vs-dark",
                "path": "./themes/catalina-dark.json"
            },
            {
                "label": "Catalina Light",
                "uiTheme": "vs",
                "path": "./themes/catalina-light.json"
            }
        ]
    }
}
