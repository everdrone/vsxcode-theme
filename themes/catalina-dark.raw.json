{
  "type": "dark",
  "tokenColors": [
    {
      "name": "Foreground",
      "scope": [
        "punctuation",
        "variable.parameter.function-call",
        "meta.function-call.arguments",
        "keyword.operator.assignment",
        "meta.object-literal.key",
        "meta.brace",
        "constant.object.key.graphql"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Less bright",
      "scope": [
        "keyword.operator.assignment",
        "meta.function.parameters keyword.operator"
      ],
      "settings": {
        "foreground": "#ffffffbb"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment",
        "comment storage.type",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6C7986"
      }
    },
    {
      "name": "Comment Notes",
      "scope": [
        "keyword.codetag.notation"
      ],
      "settings": {
        "foreground": "#92A1B1",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.declaration",
        "keyword.expressions-and-types",
        "keyword.operator.new",
        "keyword.reserved",
        "keyword.statement",
        "storage.type",
        "storage.modifier",
        "constant.language",
        "variable.language.super",
        "variable.language.this",
        "text.html entity.name.tag",
        "meta.tag entity.name.tag",
        "meta.tag support.class",
        "string.regexp keyword.other",
        "variable.language.special",
        "meta.tag.attributes keyword.operator.assignment",
        "punctuation.definition.tag",
        "keyword.schema",
        "keyword.input",
        "keyword.type",
        "support.type.property-name",
        "punctuation.support.type.property-name",
        "entity.name.tag.yaml"
      ],
      "settings": {
        "foreground": "#FC5FA3",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Bright Green",
      "scope": [
        "meta.function-call.generic",
        "meta.function-call",
        "meta.member.access",
        "meta.type.object support.type"
      ],
      "settings": {
        "foreground": "#9EF1DD"
      }
    },
    {
      "name": "Dark Green",
      "scope": [
        "variable.other.object",
        "variable.other.property",
        "variable.parameter.graphql"
      ],
      "settings": {
        "foreground": "#67B7A4"
      }
    },
    {
      "name": "Bright pink",
      "scope": [
        "entity.name.function",
        "support.function.any-method",
        "entity.other.inherited-class",
        "support.type.builtin"
      ],
      "settings": {
        "foreground": "#D0A8FF"
      }
    },
    {
      "name": "Purple",
      "scope": [
        "meta.function-call",
        "meta.member.access meta.function-call.generic",
        "meta.member.access meta.attribute",
        "meta.member.access constant.other",
        "keyword.operator.arithmetic",
        "variable.other.constant",
        "meta.function-call entity.name.function",
        "meta.objectvalues.graphql",
        "entity.name.fragment",
        "variable.fragment"
      ],
      "settings": {
        "foreground": "#A167E6"
      }
    },
    {
      "name": "Yellow",
      "scope": [
        "constant.numeric",
        "keyword.other.unit",
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#D0BF69"
      }
    },
    {
      "name": "Orange",
      "scope": [
        "string.quoted",
        "punctuation.definition.string",
        "string.unquoted.plain"
      ],
      "settings": {
        "foreground": "#FC6A5D"
      }
    },
    {
      "name": "Bright Teal",
      "scope": [
        "entity.name.type",
        "support.type"
      ],
      "settings": {
        "foreground": "#5DD8FF"
      }
    },
    {
      "name": "Dark Teal",
      "scope": [
        "meta.function.decorator",
        "punctuation.definition.decorator",
        "entity.name.function.decorator",
        "entity.scalar"
      ],
      "settings": {
        "foreground": "#41A1C0"
      }
    },
    {
      "name": "Blue",
      "scope": [
        "support.variable.magic"
      ],
      "settings": {
        "foreground": "#5482FF"
      }
    },
    {
      "name": "Brown",
      "scope": [
        "meta.tag.attributes"
      ],
      "settings": {
        "foreground": "#BF8555"
      }
    }
  ],
  "colors": {
    "editor.background": "#202026",
    "editor.foreground": "#ffffff",
    "editorLineNumber.foreground": "#ffffff33",
    "editor.lineHighlightBackground": "#2F3239",
    "editor.selectionBackground": "#3D4752",
    "focusBorder": "#82ADF3",
    "button.background": "#3C93FD",
    "list.activeSelectionBackground": "#3C93FD",
    "list.focusBackground": "#3C93FD",
    "list.hoverBackground": "#252629",
    "selection.background": "#B3D7FF",
    "list.inactiveSelectionBackground": "#515253",
    "activityBar.background": "#202026",
    "activityBar.foreground": "#ddddde",
    "statusBar.noFolderBackground": "#202026",
    "statusBar.border": "#515253",
    "statusBar.background": "#202026",
    "statusBar.foreground": "#ddddde",
    "statusBar.debuggingBackground": "#aa83e5",
    "statusBar.debuggingForeground": "#ffffff",
    "statusBarItem.hoverBackground": "#ffffff15",
    "statusBarItem.remoteForeground": "#000000",
    "statusBarItem.remoteBackground": "#69afc8"
  }
}
