{
  "type": "light",
  "tokenColors": [
    {
      "name": "Foreground",
      "scope": [
        "punctuation",
        "variable.parameter.function-call",
        "meta.function-call.arguments",
        "keyword.operator.assignment",
        "meta.object-literal.key",
        "meta.brace",
        "constant.object.key.graphql",
        "punctuation.definition.attribute.swift",
        "meta.embedded.line",
        "meta.definition.type.body meta.function-call"
      ],
      "settings": {
        "foreground": "#1c2025"
      }
    },
    {
      "name": "Less bright",
      "scope": [
        "keyword.operator.assignment",
        "meta.function.parameters keyword.operator"
      ],
      "settings": {
        "foreground": "#1c2025bb"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment",
        "comment storage.type",
        "punctuation.definition.comment",
        "string.quoted.docstring",
        "string.quoted.docstring punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#737f8b"
      }
    },
    {
      "name": "Comment Notes",
      "scope": [
        "keyword.codetag.notation"
      ],
      "settings": {
        "foreground": "#535f6b",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.declaration",
        "keyword.expressions-and-types",
        "keyword.operator.new",
        "keyword.reserved",
        "keyword.statement",
        "storage.type",
        "storage.modifier",
        "constant.language",
        "variable.language.super",
        "variable.language.this",
        "string.regexp keyword.other",
        "variable.language.special",
        "meta.tag.attributes keyword.operator.assignment",
        "meta.tag support.class",
        "text.html entity.name.tag",
        "meta.tag entity.name.tag",
        "punctuation.definition.tag",
        "keyword.schema",
        "keyword.input",
        "keyword.type",
        "support.type.property-name",
        "punctuation.support.type.property-name",
        "entity.name.tag.yaml"
      ],
      "settings": {
        "foreground": "#800080",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Bright Green",
      "scope": [
        "meta.function-call.generic",
        "meta.function-call",
        "meta.member.access",
        "meta.type.object support.type",
        "meta.definition.type.struct meta.definition.type.body meta.function-call",
        "support.class.component"
      ],
      "settings": {
        "foreground": "#31565b",
        "fontStyle": "normal"
      }
    },
    {
      "name": "Dark Green",
      "scope": [
        "variable.other.object",
        "variable.other.property",
        "variable.parameter.graphql"
      ],
      "settings": {
        "foreground": "#4f7e86"
      }
    },
    {
      "name": "Bright pink",
      "scope": [
        "entity.name.function",
        "support.function.any-method",
        "entity.other.inherited-class",
        "support.type.builtin",
        "storage.modifier.attribute",
        "meta.class.inheritance support.type"
      ],
      "settings": {
        "foreground": "#4624a9",
        "fontStyle": "normal"
      }
    },
    {
      "name": "Purple",
      "scope": [
        "meta.function-call",
        "meta.member.access meta.function-call.generic",
        "meta.member.access meta.attribute",
        "meta.member.access constant.other",
        "keyword.operator.arithmetic",
        "variable.other.constant",
        "meta.function-call entity.name.function",
        "meta.objectvalues.graphql",
        "entity.name.fragment",
        "variable.fragment",
        "meta.definition.type.body meta.function-call variable.other",
        "meta.var.expr meta.definition.variable variable.other.constant"
      ],
      "settings": {
        "foreground": "#7951b2"
      }
    },
    {
      "name": "Yellow",
      "scope": [
        "constant.numeric",
        "keyword.other.unit",
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#2729cf"
      }
    },
    {
      "name": "Preproc",
      "scope": [
        "meta.var.expr variable.other.constant",
        "constant.other.caps"
      ],
      "settings": {
        "foreground": "#643820"
      }
    },
    {
      "name": "Orange",
      "scope": [
        "string.quoted",
        "punctuation.definition.string",
        "string.unquoted.plain"
      ],
      "settings": {
        "foreground": "#c13e2a"
      }
    },
    {
      "name": "Bright Teal",
      "scope": [
        "entity.name.type",
        "support.type"
      ],
      "settings": {
        "foreground": "#296188"
      }
    },
    {
      "name": "Dark Teal",
      "scope": [
        "meta.function.decorator",
        "punctuation.definition.decorator",
        "entity.name.function.decorator",
        "entity.scalar"
      ],
      "settings": {
        "foreground": "#367aab"
      }
    },
    {
      "name": "Blue",
      "scope": [
        "support.variable.magic"
      ],
      "settings": {
        "foreground": "#1d36f5"
      }
    },
    {
      "name": "Brown",
      "scope": [
        "meta.tag.attributes"
      ],
      "settings": {
        "foreground": "#8f7223"
      }
    }
  ],
  "colors": {
